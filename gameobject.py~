class GameObject:

    def __init__(self, image, collide=False):
        self.image = image
        self.collide = collide
        self.pos = image.get_rect()
    def update():
        pass
        
    def draw(screen, x, y):
        self.pos.x = x
        self.pos.y = y
        screen.blit(self.image, self.pos)

    
